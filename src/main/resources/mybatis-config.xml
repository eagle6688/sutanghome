<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="logImpl" value="STDOUT_LOGGING" />
	</settings>
	<typeAliases>
		<package name="devutility.internal.models" />
		<package name="com.sutanghome.dao.entities" />
		<package name="com.sutanghome.dao.model" />
	</typeAliases>
	<typeHandlers>
		<typeHandler handler="com.sutanghome.common.constant.typehandlers.PaymentTypeTypeHandler" javaType="com.sutanghome.common.constant.PaymentType" />
		<typeHandler handler="com.sutanghome.common.constant.typehandlers.PaymentMediumTypeHandler" javaType="com.sutanghome.common.constant.PaymentMedium" />
		<typeHandler handler="com.sutanghome.common.constant.typehandlers.ShoppingChannelTypeHandler" javaType="com.sutanghome.common.constant.ShoppingChannel" />
	</typeHandlers>
	<mappers>
		<mapper resource="mapper/userMapper.xml" />
		<mapper resource="mapper/fundMapper.xml" />
		<mapper resource="mapper/purchaseMapper.xml" />
	</mappers>
</configuration>